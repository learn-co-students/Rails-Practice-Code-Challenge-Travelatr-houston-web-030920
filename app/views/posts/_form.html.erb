<%= form_with(model: @post) do |f| %>
    <%= f.label :blogger %><br>
    <%= f.collection_select(:blogger_id, @bloggers, :id, :name) %><br>
    <%= f.label :destination %><br>
    <%= f.collection_select(:destination_id, @destinations, :id, :name)%><br>
    <%= f.label :post_title %><br>
    <%= f.text_field :title %><br>
    <%= f.label :post_content %><br>
    <%= f.text_area :content %><br>
    <%= f.submit %>
<% end %>